{
    "manifest_version": 3,
    "name": "Kittension",
    "version": "1.0.0",
    "web_accessible_resources": [
        {
            "resources": [
                "kittensImages/*.jpg"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "kitten.js"
            ]
        }
    ],
    "icons": {
        "16": "cat.png"
    }
}