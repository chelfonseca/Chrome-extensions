{
    "manifest_version": 3,
    "name": "Coding Train Extension 2",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_title": "Click to show an alert"
      },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "icons": {
        "16": "avatar.png"
    }

}